        "@nestjs/microservices": "^10.4.20",
        "@nestjs/platform-express": "^10.4.20",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "fp-ts": "^2.16.11",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.14.5",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typeorm": "^0.3.26",
        "typia": "^9.7.2",
        "winston": "^3.17.0"
