{
    "name": "example",
    "version": "1.0.0",
    "description": "",
    "license": "ISC",
    "author": "rojiwon123",
    "type": "commonjs",
    "main": "build/main",
    "scripts": {
        "--------------------runner----------------": "",
        "start": "npm run build && NODE_ENV=development MODE=main node build/main",
        "start:main": "NODE_ENV=development MODE=main nodemon",
        "start:worker": "NODE_ENV=development MODE=worker nodemon",
        "doc": "npm run build:api && node ./doc/openapi",
        "--------------------build-----------------": "",
        "build": "rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:api": "rimraf ./doc/openapi/openapi.json && nestia swagger",
        "build:db": "rimraf ./src/lib/db/generated && prisma generate --schema=./prisma/schemas",
        "--------------------init------------------": "",
        "check": "ts-patch check",
        "prepare": "ts-patch install && typia patch && npm run build:db",
        "postinstall": "[ -f ./setup.sh ] && /bin/sh ./setup.sh || true",
        "--------------------lint------------------": "",
        "format": "prettier . -w",
        "lint": "eslint ."
    },
    "dependencies": {
        "@nestia/core": "^8.1.0",
        "@nestjs/common": "^11.1.8",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.8",
        "@nestjs/microservices": "^11.1.8",
        "@nestjs/platform-express": "^11.1.8",
        "@prisma/adapter-pg": "^6.18.0",
        "@prisma/client": "^6.18.0",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "fp-ts": "^2.16.11",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "prisma": "^6.18.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typia": "^9.7.2",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@eslint/compat": "^1.4.1",
        "@nestia/sdk": "^8.1.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/express": "^5.0.5",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.10.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "nestia": "^8.1.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "prisma-markdown": "^3.0.1",
        "rimraf": "^6.1.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.3.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3",
        "typescript-transform-paths": "^3.5.5"
    }
}
