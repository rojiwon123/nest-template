        "@types/node": "^24.3.1",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "nestia": "^8.0.7",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
