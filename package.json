    "type": "commonjs",
    "main": "build/main",
    "scripts": {
        "--------------------runner----------------": "",
        "start": "npm run build && NODE_ENV=development MODE=main node build/main",
        "start:main": "NODE_ENV=development MODE=main nodemon",
        "start:worker": "NODE_ENV=development MODE=worker nodemon",
        "--------------------build-----------------": "",
        "build": "rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:api": "rimraf ./doc/openapi/openapi.json && nestia swagger",
        "--------------------init------------------": "",
        "check": "ts-patch check",
        "prepare": "ts-patch install && typia patch",
        "postinstall": "[ -f ./setup.sh ] && /bin/sh ./setup.sh || true",
        "--------------------lint------------------": "",
        "format": "prettier . -w",
        "lint": "eslint ."
    },
    "dependencies": {
        "@nestia/core": "^8.0.7",
        "@nestjs/common": "^10.4.20",
        "@nestjs/config": "^4.0.2",
